{
  "name": "pigpio",
  "version": "3.2.4",
  "description": "Fast GPIO, PWM, servo control, state change notification, and interrupt handling on the Raspberry Pi",
  "main": "pigpio.js",
  "types": "pigpio.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "lint": "jshint *.js example/*.js test/*.js test/*/*.js",
    "test": "cd test && ./run-tests && cd ..",
    "install": "node-gyp rebuild"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fivdi/pigpio.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "nan": "^2.14.1"
  },
  "devDependencies": {
    "jshint": "^2.11.0"
  },
  "keywords": [
    "gpio",
    "pwm",
    "servo",
    "iot",
    "interrupt",
    "raspberry",
    "raspi",
    "rpi",
    "pi"
  ],
  "author": "fivdi",
  "license": "MIT",
  "gypfile": true
}
